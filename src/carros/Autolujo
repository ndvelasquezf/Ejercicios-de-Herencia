public class Autolujo extends Vehiculo {
    
    public int pasajeros;
    
    public Autolujo(int pasajeros, int seriemotor, String marca, int a침o, double precio) {
        super(seriemotor, marca, a침o, precio);
        this.pasajeros = pasajeros;
    }

    @Override
    public String toString() {
        return "Auto de lujo: " +super.toString()+ ", numero de pasajeros= " + pasajeros ;
    }
}
